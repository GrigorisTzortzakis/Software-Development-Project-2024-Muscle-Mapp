<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical">
	<LinearLayout
		android:id="@+id/linear1"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:padding="8dp"
		android:background="#0D0D0D"
		android:gravity="center_horizontal"
		android:orientation="vertical">
		<LinearLayout
			android:id="@+id/linear8"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="#0D0D0D"
			android:gravity="center_vertical"
			android:orientation="horizontal"
			android:weightSum="1">
			<ImageView
				android:id="@+id/imageview5"
				android:layout_width="160dp"
				android:layout_height="75dp"
				android:src="@drawable/logo11"
				android:scaleType="centerInside" />
			<LinearLayout
				android:id="@+id/linear9"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="right|center_vertical"
				android:orientation="horizontal">
				<androidx.cardview.widget.CardView
					android:id="@+id/notification"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="10dp"
					android:background="#00E53535"
					android:orientation="vertical"
					android:padding="4dp"
					app:cardBackgroundColor="#1A1A1A"
					app:cardCornerRadius="50dp"
					app:cardElevation="10dp">

					<ImageView
						android:id="@+id/notbell"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="12dp"
						android:scaleType="centerCrop"
						android:src="@drawable/notification" />
				</androidx.cardview.widget.CardView>
				<androidx.cardview.widget.CardView
					android:id="@+id/notification"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginEnd="10dp"
					android:background="#00E53535"
					android:orientation="vertical"
					android:padding="4dp"
					app:cardBackgroundColor="#1A1A1A"
					app:cardCornerRadius="50dp"
					app:cardElevation="10dp">

					<ImageView
						android:id="@+id/notbell"
						android:layout_width="50dp"
						android:layout_height="50dp"
						android:padding="12dp"
						android:scaleType="centerInside"
						android:src="@drawable/fav" />
				</androidx.cardview.widget.CardView>
				<ImageView
					android:id="@+id/imageview39"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:src="@drawable/profpic"
					android:scaleType="centerCrop"
					tools:ignore="DuplicateIds" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear45"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal" />
		<LinearLayout
			android:id="@+id/linear32"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginLeft="10dp"
			android:layout_marginTop="20dp"
			android:layout_marginRight="10dp"
			android:layout_marginBottom="20dp"
			android:background="#0D0D0D"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear41"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:background="#0D0D0D"
				android:gravity="center_vertical"
				android:orientation="horizontal">
				<TextView
					android:id="@+id/textview33"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:text="Active memberships"
					android:textSize="25sp"
					android:textColor="#FFFFFF" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear87"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear89"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/imageview39"
					android:layout_width="200dp"
					android:layout_height="300dp"
					android:src="@drawable/palladion"
					android:scaleType="centerCrop" />
			</LinearLayout>
			<LinearLayout
				android:id="@+id/linear90"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_marginLeft="10dp"
				android:orientation="vertical">
				<ImageView
					android:id="@+id/imageview40"
					android:layout_width="200dp"
					android:layout_height="300dp"
					android:src="@drawable/alterlifecard"
					android:scaleType="centerCrop" />
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear88"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:padding="8dp"
			android:orientation="horizontal">
			<LinearLayout
				android:id="@+id/linear94"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:gravity="center_horizontal|center_vertical"
				android:orientation="horizontal">
				<LinearLayout
					android:id="@+id/linear95"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:orientation="vertical">
					<ImageView
						android:id="@+id/imageview43"
						android:layout_width="200dp"
						android:layout_height="300dp"
						android:src="@drawable/yava"
						android:scaleType="centerCrop" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear96"
					android:layout_width="wrap_content"
					android:layout_height="match_parent"
					android:layout_marginLeft="10dp"
					android:orientation="vertical">
					<ImageView
						android:id="@+id/imageview44"
						android:layout_width="200dp"
						android:layout_height="300dp"
						android:src="@drawable/workouthall"
						android:scaleType="centerCrop" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
		<LinearLayout
			android:id="@+id/linear79"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:layout_marginTop="100dp"
			android:gravity="center_horizontal|center_vertical"
			android:orientation="horizontal"
			android:padding="8dp">

			<LinearLayout
				android:id="@+id/linear83"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_marginRight="60dp"
				android:orientation="vertical">

				<ImageView
					android:id="@+id/imageview35"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:scaleType="centerCrop"
					android:src="@drawable/home" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/linear84"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_marginRight="60dp"
				android:orientation="vertical">

				<ImageView
					android:id="@+id/imageview36"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:scaleType="centerCrop"
					android:src="@drawable/calendar" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/linear85"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:layout_marginRight="60dp"
				android:orientation="vertical">

				<ImageView
					android:id="@+id/imageview37"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:scaleType="centerCrop"
					android:src="@drawable/location_1" />
			</LinearLayout>

			<LinearLayout
				android:id="@+id/linear86"
				android:layout_width="wrap_content"
				android:layout_height="match_parent"
				android:orientation="vertical">

				<ImageView
					android:id="@+id/imageview38"
					android:layout_width="50dp"
					android:layout_height="50dp"
					android:scaleType="centerCrop"
					android:src="@drawable/bytesize_car" />
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
